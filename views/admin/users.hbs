<div class="content">
  <section class="price-area section-gap pb-4">
    <div class="row">
      <div class="col-md-12">
        <div class="card">
          <div class="card-header ">
            <h5 class="card-title">Users</h5>

          </div>
          <div class="card-body ">
            <div class="row">
              <table class="table display ml-3 " id="table_id">
                <thead>
                  <tr>
                    <th scope="col">User Name</th>
                    <th scope="col">Name</th>
                    <th scope="col">Address</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each client}}
                  <tr>
                    <th>{{this.username}}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.number}}<br>
                      {{this.email}}<br>
                      {{this.city}}<br>
                      {{this.country}}<br>
                      {{this.zip_code}}<br>
                    </td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>

